<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="640.0" prefWidth="640.0" style="-fx-background-image: url(&quot;images/background_image.jpg&quot;);"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Controller.HowToPlayController">

   <HBox layoutX="10.0" layoutY="210.0" prefHeight="56.0" prefWidth="640.0">
      <VBox.margin>
         <Insets top="200.0"/>
      </VBox.margin>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="1118.0" text="HOW TO PLAY"
             textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets/>
         </HBox.margin>
      </Label>
   </HBox>
   <HBox prefHeight="56.0" prefWidth="640.0">
      <VBox.margin>
         <Insets/>
      </VBox.margin>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="1118.0"
             text="MOVEMENTS: W,A,S,D OR ARROW KEYS" textAlignment="CENTER" textFill="WHITE">
         <HBox.margin>
            <Insets/>
         </HBox.margin>
         <font>
            <Font size="22.0"/>
         </font>
      </Label>
   </HBox>
   <HBox layoutX="10.0" layoutY="210.0" prefHeight="39.0" prefWidth="640.0">
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="822.0" text="ATTACK: SPACE"
             textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets/>
         </HBox.margin>
      </Label>
   </HBox>
   <Button onAction="#HandleBackButton" prefHeight="20.0" prefWidth="140.0" text="Back">
      <effect>
         <DropShadow color="#0038ff"/>
      </effect>
      <VBox.margin>
         <Insets left="237.0" top="30.0"/>
      </VBox.margin>
   </Button>
</VBox>
